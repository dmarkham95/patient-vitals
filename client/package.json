		"tailwindcss": "^1.2.0",
		"typescript": "^3.7.4",
		"yup": "^0.28.0"
	},
	"scripts": {
		"start": "npm run tailwind && cross-env NODE_ENV=development && react-scripts start",
		"build": "npm run tailwind && react-scripts build",
		"build_prod": "npm run tailwind && cross-env NODE_ENV=development && cross-env GENERATE_SOURCEMAP=false && react-scripts build",
		"tailwind": "tailwind build ./src/styles/tailwind-config.css -c ./tailwind.config.js -o ./src/styles/tailwind.css",
		"purge-tailwind": "node ./purge-tailwindcss.js",
		"test": "react-scripts test",
		"envcmd": "env-cmd -f .env.development --verbose",
		"eject": "react-scripts eject",
		"cypress:run": "cypress run",
		"cypress:open": "cypress open",
		"lint:fix": "eslint \"./src/**/*.{ts,tsx}\"",
		"lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
		"build-stats": "npm run build --stats",
		"bundle-report": "npm run build-stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
		"predeploy": "npm run build",
		"deploy": "aws s3 --profile markham_tech_admin sync build/ s3://patient-vitals-client --delete",
		"deploy:dev": "aws s3 sync build/ s3://patient-vitals-client-dev --delete"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@bahmutov/add-typescript-to-cypress": "^2.1.2",
		"@types/lodash": "^4.14.144",
		"@types/react": "^16.9.34",
		"@typescript-eslint/eslint-plugin": "^2.28.0",
		"@typescript-eslint/parser": "^2.28.0",
		"cross-env": "^7.0.2",
		"cypress": "^3.4.1",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-airbnb-typescript": "^6.3.1",
		"eslint-config-prettier": "^6.10.1",
		"eslint-config-react": "^1.1.7",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^1.7.0",
		"prettier": "^1.19.1",
		"purgecss": "^2.1.2",
		"env-cmd": "8.0.2"
	}
}
