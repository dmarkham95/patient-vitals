		"easy-peasy": "^3.3.0",
		"lodash": "^4.17.15",
		"react": "^16.12.0",
		"react-chartjs-2": "^2.9.0",
		"react-dom": "^16.12.0",
		"react-hook-form": "^4.5.2",
		"react-router": "^5.1.2",
